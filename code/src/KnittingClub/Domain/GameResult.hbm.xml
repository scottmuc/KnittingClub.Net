<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="KnittingClub" namespace="KnittingClub.Domain">
  <class name="KnittingClub.Domain.GameResult, KnittingClub" table="KC_GameResults">
    <id name="Id" column="ResultId" unsaved-value="0">
      <generator class="native" />
    </id>

    <many-to-one name="Player" class="KnittingClub.Domain.Player, KnittingClub" lazy="false" column="PlayerId" />
    <many-to-one name="KnockedOutBy" class="KnittingClub.Domain.Player, KnittingClub" not-null="false" lazy="false" column="KnockedOutById" />
    
    <property name="Place" column="Place" not-null="true" />

  </class>
</hibernate-mapping>
