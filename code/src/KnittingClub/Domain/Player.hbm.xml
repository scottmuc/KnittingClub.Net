<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="KnittingClub" namespace="KnittingClub.Domain">
  <class name="Player" table="KC_Players">
    <id name="id" access="field" column="PlayerId" unsaved-value="0">
      <generator class="native" />
    </id>

    <property name="FirstName" column="FirstName" not-null="true" />
    <property name="LastName" column="LastName" not-null="true" />
    <property name="NickName" column="NickName" not-null="true" />
    
  </class>
</hibernate-mapping>
